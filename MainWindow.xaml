            <TextBox x:Name="startDateTB" TextWrapping="Wrap" FontSize="24" Height="41" VerticalAlignment="Bottom" DockPanel.Dock="Left" Width="206" BorderThickness="0,0,0,2" BorderBrush="#FF632F63" HorizontalAlignment="Center"/>
            <Button x:Name="SaveImageButton" Content="Export to PDF" Width="322" Height="41" FontSize="24" BorderBrush="#FF632F63" BorderThickness="2,0,0,2" DockPanel.Dock="Left"/>
            <Button x:Name="EditAttendantsButton" Content="Edit Attendants" Width="326" DockPanel.Dock="Left" Height="41" FontSize="24" BorderThickness="2,0,0,2" BorderBrush="#FF632F63" Click="EditAttendantsClick"/>
        </DockPanel>
    </Grid>
</Window>
